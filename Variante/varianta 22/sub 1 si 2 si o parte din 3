int n=3, m=4, a[10][10],k;
    k=n*m;
    for(int i=1; i<=m; i++)
        for(int j=1; j<=n; j++)
        {
            a[i][j]=k;
            k--;
        }

    for(int i=1; i<=m; i++, cout<<endl)
        for(int j=1; j<=n; j++)
            cout<<a[i][j]<<" ";
  
